@model List<Domain.Entities.Category>

@{
    ViewData["Title"] = "Buy sell easy";
}
<div>
    <h1>Categories</h1>
    <ul>
        <li><a href="/categories">All categories</a></li>
        @foreach (var category in Model)
        {
            <li>@category.Name</li>
        }
    </ul>
    
    <h1>Products</h1>
    <div>
            <a href="/products">Products</a>
        </div>
    </div>
